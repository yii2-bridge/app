{
  "env": {
    "node": true,
    "browser": true,
    "es6": true,
    "jquery": true
  },
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "rules": {
    "quotes": [ 1, "single" ],
    "comma-dangle": [ 2, "never" ],
    "no-extra-semi": [ 2 ],
    "no-multi-spaces": [ 1 ]
  }
}
